# AWS Configuration
AWS_REGION=us-west-2
AWS_ACCOUNT_ID=your-account-id

# AWS Cognito (for authentication)
# These will be generated by setup_gateway.py
COGNITO_USER_POOL_ID=us-west-2_xxxxxxxxx
COGNITO_CLIENT_ID=your-client-id
COGNITO_CLIENT_SECRET=your-client-secret

# Demo Mode (set to false for production with real auth)
DEMO_MODE=true

# AgentCore Memory IDs
# These will be generated by setup_memory.py
MEMORY_STM_ID=your-stm-memory-id
MEMORY_LTM_ID=your-ltm-memory-id

# AgentCore Gateway Configuration
# These will be generated by setup_gateway.py
GATEWAY_URL=https://your-gateway-url
GATEWAY_ID=your-gateway-id
GATEWAY_ACCESS_TOKEN=your-gateway-access-token

# API Keys for Real-World APIs (Optional - for real flight/hotel data)
AMADEUS_API_KEY=your-amadeus-api-key
AMADEUS_API_SECRET=your-amadeus-api-secret
BOOKING_API_KEY=your-booking-api-key

# LLM Configuration - Using Latest Bedrock Models (No API keys needed!)
# All agents use Bedrock - just AWS credentials required

# Latest Models (Recommended - Best Performance)
ORCHESTRATOR_MODEL=anthropic.claude-sonnet-4-5-20250929-v1:0
FLIGHT_AGENT_MODEL=anthropic.claude-sonnet-4-5-20250929-v1:0
HOTEL_AGENT_MODEL=anthropic.claude-sonnet-4-5-20250929-v1:0
VISA_AGENT_MODEL=anthropic.claude-haiku-4-5-20251001-v1:0
ITINERARY_AGENT_MODEL=anthropic.claude-sonnet-4-5-20250929-v1:0

# Alternative Models (uncomment to use)
# Claude Opus 4.5 (Most capable, highest cost)
# ORCHESTRATOR_MODEL=anthropic.claude-opus-4-5-20251101-v1:0

# Claude Haiku 4.5 (Fast, cost-effective)
# FLIGHT_AGENT_MODEL=anthropic.claude-haiku-4-5-20251001-v1:0
# HOTEL_AGENT_MODEL=anthropic.claude-haiku-4-5-20251001-v1:0

# Amazon Nova 2 (Latest AWS native models)
# ORCHESTRATOR_MODEL=amazon.nova-2-lite-v1:0
# FLIGHT_AGENT_MODEL=amazon.nova-premier-v1:0

# DeepSeek R1 (Reasoning model)
# ORCHESTRATOR_MODEL=deepseek.r1-v1:0

# Meta Llama 3.3 (Open source, cost-effective)
# FLIGHT_AGENT_MODEL=meta.llama3-3-70b-instruct-v1:0

# Deployment Configuration
STACK_PREFIX=umrah-trip
S3_BUCKET_PREFIX=umrah-trip-creator
